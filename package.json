{
  "name": "next-starter-strict",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true pnpm run build",
    "start": "next start",
    "clean": "git clean --interactive -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
    "lint": "npm-run-all --parallel lint:js+ts lint:format lint:types",
    "lint:js+ts": "eslint --ext .js,.jsx,.mjs,.ts,.tsx,.mts --ignore-path .gitignore .",
    "lint:format": "prettier --check .",
    "lint:types": "tsc --noEmit",
    "lint:fix": "npm-run-all --serial lint:fix:*",
    "lint:fix:js+ts": "pnpm run lint:js+ts --fix",
    "lint:fix:format": "prettier --write ."
  },
  "dependencies": {
    "@heroicons/react": "2.0.12",
    "@next/bundle-analyzer": "12.3.1",
    "clsx": "1.2.1",
    "focus-visible": "5.2.0",
    "next": "12.3.1",
    "postcss-import": "15.0.0",
    "postcss-preset-env": "7.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.2.1"
  },
  "devDependencies": {
    "@tsconfig/next": "1.0.3",
    "@types/node": "16.11.68",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.40.1",
    "@typescript-eslint/parser": "5.40.1",
    "cross-env": "7.0.3",
    "eslint": "8.25.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "eslint-plugin-react-etc": "2.0.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.1",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "typescript": "4.8.4"
  },
  "packageManager": "pnpm@7.13.6",
  "nano-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss"
      ]
    }
  }
}
