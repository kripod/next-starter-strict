{
  "name": "next-starter-strict",
  "private": true,
  "scripts": {
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true pnpm run build",
    "clean": "git clean --interactive -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
    "dev": "next dev",
    "fix": "run-s fix:*",
    "fix:eslint": "pnpm run lint:eslint --fix",
    "fix:prettier": "pnpm run lint:prettier --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint --ext .ts,.tsx,.js,.jsx --ignore-path .gitignore .",
    "lint:prettier": "prettier --check .",
    "lint:typescript": "tsc",
    "prepare": "husky install",
    "start": "next start"
  },
  "dependencies": {
    "@heroicons/react": "2.0.11",
    "@next/bundle-analyzer": "12.3.0",
    "@react-aria/live-announcer": "3.1.1",
    "clsx": "1.2.1",
    "focus-visible": "5.2.0",
    "next": "12.3.0",
    "postcss-import": "15.0.0",
    "postcss-preset-env": "7.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.1.8"
  },
  "devDependencies": {
    "@tsconfig/next": "1.0.2",
    "@types/node": "16.11.58",
    "@types/react": "18.0.19",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "cross-env": "7.0.3",
    "eslint": "8.23.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-next": "12.3.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-etc": "2.0.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.1",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "prettier-plugin-tailwindcss": "0.1.13",
    "typescript": "4.8.3"
  },
  "packageManager": "pnpm@7.11.0",
  "nano-staged": {
    "*": "prettier --write --ignore-unknown",
    "*.{ts,tsx,js,jsx}": "eslint --fix"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss"
      ]
    }
  }
}
